                <para style="P1">
                  <font color="white"> </font>
                </para>
              </td>
              <td>
                <para style="P3">[['$ ' + moneyfmt( '%.2f' % l.price_unit) ]]</para>
              </td>
              <td>
                <para style="P3">[[ l.discount or removeParentNode('para')]] %</para>
              </td>
              <td>
                <para style="P3">[['$ ' +moneyfmt( '%.2f' %l.price_subtotal) ]]</para>
