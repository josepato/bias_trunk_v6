            <tr>
              <td>
                <para style="P2">[[repeatIn(o.invoice_line,'l') ]] </para>
                <para style="P2">[[ moneyfmt( '%.2f'%l.quantity) ]]</para>
              </td>
              <td>
                <para style="P2">[[ l.uos_id.name]]</para>
              </td>
              <td>
