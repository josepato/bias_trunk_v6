    <para style="P20">[[ repeatIn(objects,'o') ]]</para>
    <para style="P11">[[ setLang(o.partner_id.lang) ]]</para>
    <blockTable colWidths="413.0,85.0" style="Table11">
      <tr>
        <td>
          <para style="P16">Comprobante de [[ tipoDeComprobante(o) ]]</para>
          <para style="P33">Facturado A: [[ o.address_invoice_id.partner_id.name ]]</para>
        </td>
        <td>
          <para style="P14">Folio:</para>
          <para style="P14">[[ o.number ]]</para>
        </td>
      </tr>
      <tr>
        <td>
          <para style="P10">R.F.C.: [[ o.address_invoice_id.partner_id.vat ]]</para>
          <para style="P9">Dirección:[[ o.address_invoice_id.street[0:45] ]] Colonia: [[ o.address_invoice_id.street2 or '' ]]</para>
          <para style="P9">[[ o.address_invoice_id.city or '' ]], [[ o.address_invoice_id.state_id and o.address_invoice_id.state_id.name or '' ]], [[ o.address_invoice_id.zip or '' ]]</para>
          <para style="P9">[[ o.address_invoice_id.country_id.name or '' ]]</para>
          <para style="P9">Tel: [[ o.address_invoice_id.phone or removeParentNode('para')]]</para>
        </td>
        <td>
          <para style="P15">
            <font face="Helvetica">Fecha:</font>
          </para>
          <para style="P18">[[ date_sp(o.sign_date,2) ]]</para>
        </td>
      </tr>
    </blockTable>
    <para style="P25">
       <font color="white"> </font>
    </para>
    <blockTable colWidths="50.0,448.0" style="Table11">
      <tr>
        <td>
          <para style="P23">Descripción:</para>
        </td>
        <td>
          <para style="P25">[[ o.name ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="50.0,65.0,61.0,155.0,87.0,80.0" style="Table13">
